{
  "compilerOptions": {
  
    "target": "es2016",                                 
    "module": "commonjs",                                
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                     
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
  },     
  "include": ["src","__tests__/**.ts"],
  "exclude": ["__tests__/**.ts"]                      
}
